{
  "manifest_version": 2,
  "name": "Feed Blocker",
  "version": "0.1",
  "content_scripts": [
	{
	  "matches": [ "*://*/*" ],
	  "js": [ "content.js" ]
	}
  ],
  "background": {
	"scripts": ["background.js"]
  },
  "permissions": [
	"tabs",
	"https://www.youtube.com/*"
  ],
  "browser_action": {
	"default_title": "Feed Blocker",
	"default_popup": "popup.html"
  }
}
